{
  "name": "vue-ts-tailwind",
  "version": "1.0.0",
  "description": "Testing project",
  "author": "Harry Tran",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "yarn lint && nuxt-ts generate",
    "lint:eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:eslint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "test": "jest"
  },
  "engines": {
    "node": ">=10.19.0"
  },
  "gitHooks": {
    "pre-commit": "ls-lint && cross-env PRE_COMMIT=true lint-staged",
    "commit-msg": "node scripts/verifyCommitMsg.js"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.4.5",
    "@nuxtjs/axios": "5.9.7",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "compression": "1.7.4",
    "cross-env": "5.2.1",
    "express": "4.17.1",
    "nuxt": "2.12.2",
    "nuxt-property-decorator": "2.5.1"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "1.8.1",
    "@nuxt/typescript-build": "0.6.5",
    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/stylelint-module": "3.2.2",
    "@nuxtjs/tailwindcss": "1.4.0",
    "@nuxtjs/vuetify": "1.11.0",
    "@types/compression": "1.7.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "chalk": "4.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-nuxt": "0.5.2",
    "eslint-plugin-prettier": "3.1.3",
    "imagemin-lint-staged": "0.4.0",
    "jest": "24.9.0",
    "lint-staged": "10.1.6",
    "node-sass": "4.13.1",
    "nodemon": "2.0.3",
    "prettier": "2.0.4",
    "sass-loader": "8.0.2",
    "stylelint": "13.3.2",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.17.0",
    "ts-jest": "25.4.0",
    "vue-jest": "4.0.0-beta.2",
    "yorkie": "2.0.0"
  }
}
